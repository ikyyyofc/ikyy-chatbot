{
  "cleanUrls": true,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "redirects": [
    {
      "source": "/api/(.*)",
      "destination": "https://ikyy-ikyy-chatbot.hf.space/api/$1",
      "statusCode": 307
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/"
    }
  ]
}
